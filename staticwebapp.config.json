{
  "routes": [
    {
      "route": "/",
      "serve": "/taskpane.html"
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/taskpane.html"
  },
  "responseOverrides": {
    "401": {
      "redirect": "/",
      "statusCode": 302
    }
  }
}


